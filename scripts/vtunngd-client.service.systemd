[Unit]
Description=Simple network tunneling service (vtunngd)
After=network.target
Requires=network.target

[Service]
Type=simple
ExecStart=/usr/bin/vtunngd -n %i ${HOSTNAME}
EnvironmentFile=-/etc/vtunngd/%i.env
Restart=always
RestartSec=5
User=root
Group=root

[Install]
WantedBy=multi-user.target

